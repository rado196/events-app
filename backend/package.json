{
  "name": "events-app-backend",
  "version": "1.0.0",
  "description": "Events app server-side project.",
  "author": "Rado Blrtsyan",
  "license": "ISC",
  "scripts": {
    "start": "nodemon ./src/http.js",
    "format": "npm run tool:code:format",
    "lint": "npm run tool:code:lint:dev",
    "db:sync": "npm run db:migrate && npm run db:seed",
    "db:migrate": "bash ./scripts/app/sequelize.sh db:migrate",
    "db:migrate:undo": "bash ./scripts/app/sequelize.sh db:migrate:undo",
    "db:migrate:undo-all": "bash ./scripts/app/sequelize.sh db:migrate:undo:all",
    "db:seed": "bash ./scripts/app/sequelize.sh db:seed:all",
    "db:seed:undo": "bash ./scripts/app/sequelize.sh db:seed:undo",
    "db:seed:undo-all": "bash ./scripts/app/sequelize.sh db:seed:undo:all",
    "tool:code:lint:dev": "bash ./scripts/tools/eslint-execute.sh dev",
    "tool:code:lint:prod": "bash ./scripts/tools/eslint-execute.sh prod",
    "tool:code:format": "bash ./scripts/tools/format-code.sh"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-list-routes": "^1.1.5",
    "fast-xml-parser": "^4.2.6",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@babel/plugin-proposal-export-default-from": "^7.22.5",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.22.6",
    "@babel/plugin-transform-modules-umd": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/plugin-transform-typescript": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@babel/runtime": "^7.22.6",
    "babel-preset-minify": "^0.5.2",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^48.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize-cli": "^6.6.1"
  }
}
